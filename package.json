{
  "name": "libercrypt",
  "version": "1.0.0",
  "description": "website",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start_prod --prefix client",
    "client_dev": "npm start_dev --prefix client"
    "dev": "concurrently \"npm run server\" \"npm run client_dev\"",
    "prod": "concurrently \"npm run server\" \"npm run client\"",
    "heroku_postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build_prod -- prefix client"
  },
  "author": "James McNeilan",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": "14.16.0"
  }
}
