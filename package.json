{
  "name": "libercrypt",
  "version": "1.0.0",
  "description": "website",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix public",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client_craco": "npm run start_craco --prefix public",
    "client_react": "npm run start_react --prefix public",
    "dev": "concurrently \"npm run server\" \"npm run client_craco\"",
    "prod": "NPM_CONFIG_PRODUCTION=true NODE_ENV='production' npm run server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix public && npm run build_craco --prefix public"
  },
  "author": "James McNeilan",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0",
    "nodemailer": "^6.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  }
}
